<%=provide(:title, 'New Product')%>
<h1> New Product - PG Tiny Web App</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@product) do |f| %>
        <%= render(partial: 'form', locals: {f: f})%>
        <%= f.submit("Create product",
                     class: "btn btn-large btn-primary") %>
    <% end %>
  </div>
</div>


