      <nav>
        <ul class="nav pull-right">
          <li><%=link_to("Home", cache_path)%></li>
          <li><%=link_to("About", about_path)%></li>
          <li><%=link_to("Products", '#')%></li>
          <li><%=link_to("Users", '#')%></li>
